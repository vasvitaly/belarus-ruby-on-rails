<% if user_signed_in? %>
  <li><%= link_to('Edit registration', edit_user_registration_path) %></li>
<% else %>
  <li><%= link_to 'Sign up', new_user_registration_path  %></li>
  <li><%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) %></li>
  <li><%= link_to "Sign in with Vkontakte", user_omniauth_authorize_path(:vkontakte) %></li>
  <li><%= link_to "Sign in with GoogleApps", user_omniauth_authorize_path(:google_apps) %></li>
<% end %>
